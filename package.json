{
  "name": "github",
  "version": "1.0.0",
  "description": "Multi-folder landing page generator for education web apps",
  "main": "./scripts/build-landing-pages.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build:css": "node ./node_modules/tailwindcss/lib/cli.js -c tailwind.config.js -i src/styles.css -o dist/assets/tailwind.css --minify",
    "build:pages": "ts-node scripts/build-landing-pages.ts",
    "build": "npm run clean && npm run build:css && npm run build:pages",
    "dev": "ts-node scripts/build-landing-pages.ts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keithsflau/profile.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/keithsflau/profile/issues"
  },
  "homepage": "https://github.com/keithsflau/profile#readme",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
